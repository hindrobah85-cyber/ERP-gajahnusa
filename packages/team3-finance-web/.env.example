# Team 3 Finance Web App Environment Variables
VITE_APP_TITLE=ERP Finance Management
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_ML_HUB_URL=http://localhost:8000/ml

# Authentication
VITE_JWT_SECRET=your_jwt_secret_key_here_team3_finance
VITE_TOKEN_STORAGE_KEY=erp_finance_token

# External Services
VITE_PAYMENT_GATEWAY_URL=https://api.payment-provider.com
VITE_BANK_API_URL=https://api.bank-provider.com
VITE_TAX_API_URL=https://api.tax-provider.com

# Features Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_EXPORT=true

# UI Configuration
VITE_DEFAULT_CURRENCY=USD
VITE_DEFAULT_LOCALE=en-US
VITE_THEME_MODE=light

# Development
VITE_DEBUG_MODE=true
VITE_API_MOCK=false